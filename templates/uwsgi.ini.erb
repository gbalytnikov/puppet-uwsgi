#
# This file is managed by puppet class 'uwsgi'
#
# uWSGI main process configuration
#
[uwsgi]
socket = <%= @socket %>
pidfile = <%= @pidfile %>
emperor = <%= @app_directory %>
emperor-tyrant = true
master = true
no-orphans = true
autoload = true
log-date = true
logto = <%= @log_file %>
<% if @service_provider != "upstart" %>
daemonize = <%= @log_file %>
<% end %>
<%
if @emperor_options
  @emperor_options.sort.each do |key, value|
-%>
<%= key %> = <%= value%>
<%
  end
end
-%>
